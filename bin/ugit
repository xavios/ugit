#!/home/andras/dev/andras/ugit/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'ugit','console_scripts','ugit'
__requires__ = 'ugit'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('ugit', 'console_scripts', 'ugit')()
    )
